{
  "name": "write-always",
  "version": "0.0.0",
  "description": "A community built around creative writing and sharing stories.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --open",
    "build": "NODE_ENV=production; webpack --progress",
    "lint": "eslint --ext .js,.json,.vue .",
    "fix": "eslint --ext .js,.json,.vue . --fix",
    "commit": "commit",
    "todo": "leasot . -xSIi \"node_modules,public\"",
    "todo-generate": "leasot . -xSIi \"node_modules,public\" --reporter markdown > TODO.md"
  },
  "_moduleAliases": {
    "@root": "./",
    "@app": "app/",
    "@tools": "tools/",
    "@config": "app/config/"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run todo-generate && git add TODO.md && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaJoker29/write-always.git"
  },
  "author": "Dewitt Buckingham (http://zerodaedalus.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DaJoker29/write-always/issues"
  },
  "homepage": "https://github.com/DaJoker29/write-always#readme",
  "dependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@commitlint/prompt-cli": "7.2.1",
    "babel-eslint": "10.0.1",
    "callsite": "1.0.0",
    "css-loader": "2.1.0",
    "debug": "4.1.1",
    "deepmerge": "3.0.0",
    "dotenv": "6.2.0",
    "eslint": "5.11.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-import-order-autofix": "0.8.3",
    "eslint-plugin-json": "1.3.2",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-vue": "5.0.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.2.1",
    "leasot": "6.6.2",
    "lint-staged": "8.1.0",
    "module-alias": "2.1.0",
    "prettier": "1.15.3",
    "verror": "1.10.0",
    "vue": "2.5.21",
    "vue-loader": "15.4.2",
    "vue-router": "3.0.2",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.5.21",
    "vuex": "3.0.1",
    "webpack": "4.28.2",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.1.5"
  }
}
