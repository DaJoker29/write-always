{
  "name": "write-always",
  "version": "0.4.1",
  "description": "A community built around creative writing and sharing stories.",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --timeout 15000",
    "coverage": "nyc --check-coverage npm test",
    "start": "nodemon index.js --config nodemon.json",
    "build": "NODE_ENV=production; webpack --progress",
    "lint": "eslint --ext .js,.json,.vue .",
    "fix": "eslint --ext .js,.json,.vue . --fix",
    "release": "standard-version"
  },
  "nyc": {
    "include": [
      "app/server/routes",
      "app/server/middleware",
      "app/server/authentication"
    ],
    "reporter": [
      "lcov",
      "html",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "bin": {
    "cli": "tools/cli.js"
  },
  "_moduleAliases": {
    "@root": "./",
    "@app": "./app/",
    "@tools": "./tools/",
    "@client": "./app/client/",
    "@server": "./app/server/",
    "@config": "./app/config/"
  },
  "standard-version": {
    "scripts": {
      "prerelease": "git checkout master; git pull origin master",
      "posttag": "git push --follow-tags origin master"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npm run coverage"
    }
  },
  "lint-staged": {
    "*.{js,json,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "IE 10"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaJoker29/write-always.git"
  },
  "author": "Dewitt Buckingham (https://zerodaedalus.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DaJoker29/write-always/issues"
  },
  "homepage": "https://github.com/DaJoker29/write-always#readme",
  "dependencies": {
    "@babel/core": "7.2.2",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.2.3",
    "@babel/register": "7.0.0",
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "axios": "0.18.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-istanbul": "5.1.0",
    "callsite": "1.0.0",
    "chai": "4.2.0",
    "connect-history-api-fallback": "1.6.0",
    "core-js": "2.6.2",
    "css-loader": "2.1.0",
    "debug": "4.1.1",
    "deepmerge": "3.0.0",
    "dotenv": "6.2.0",
    "encodeurl": "1.0.2",
    "eslint": "5.11.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-json": "1.3.2",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-vue": "5.0.0",
    "fs-extra": "7.0.1",
    "helmet": "3.15.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.2.1",
    "jsonwebtoken": "8.4.0",
    "lint-staged": "8.1.0",
    "method-override": "3.0.0",
    "mocha": "5.2.0",
    "module-alias": "2.1.0",
    "moment": "2.23.0",
    "mongoose": "5.4.2",
    "mongoose-lean-virtuals": "0.3.4",
    "mongoose-unique-validator": "2.0.2",
    "morgan": "1.9.1",
    "morgan-debug": "2.0.0",
    "nodemon": "1.18.9",
    "nprogress": "0.2.0",
    "nyc": "13.1.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "prettier": "1.15.3",
    "random-token": "0.0.8",
    "shortid": "2.2.14",
    "standard-version": "4.4.0",
    "supertest": "3.4.2",
    "thirty-two": "1.0.2",
    "verror": "1.10.0",
    "vue": "2.5.21",
    "vue-autosize": "1.0.2",
    "vue-loader": "15.4.2",
    "vue-router": "3.0.2",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.5.21",
    "vuex": "3.0.1",
    "webpack": "4.28.2",
    "webpack-cli": "3.1.2",
    "webpack-dev-middleware": "3.5.0",
    "webpack-dev-server": "3.1.14",
    "webpack-hot-middleware": "2.24.3",
    "webpack-merge": "4.1.5"
  }
}
